{
  "id": "copilot-for-hbuilderx",
  "name": "copilot-for-hbuilderx",
  "description": "Copilot for HBuilderX",
  "displayName": "Copilot for HBuilderX",
  "version": "0.1.0",
  "publisher": "zhetengbiji",
  "engines": {
    "HBuilderX": "^2.7.0",
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "main": "./out/index.js",
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "copilot.status",
        "title": "Copilot Status"
      }
    ],
    "menus": {
      
    },
    "icons": {
      "copilot-enable": {
        "description": "copilot enable",
        "default": {
          "fontPath": "iconfont.woff",
          "fontCharacter": "\\E60F"
        }
      },
      "copilot-loading": {
        "description": "copilot loading",
        "default": {
          "fontPath": "iconfont.woff",
          "fontCharacter": "\\E627"
        }
      },
      "copilot-warning": {
        "description": "copilot warning",
        "default": {
          "fontPath": "iconfont.woff",
          "fontCharacter": "\\E62C"
        }
      },
      "copilot-disable": {
        "description": "copilot disable",
        "default": {
          "fontPath": "iconfont.woff",
          "fontCharacter": "\\E62B"
        }
      }
    }
  },
  "extensionDependencies": [],
  "dependencies": {
    "json-rpc-2.0": "^1.6.0",
    "vscode-jsonrpc": "^8.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.10",
    "@types/vscode": "^1.83.1",
    "typescript": "^5.2.2"
  }
}
